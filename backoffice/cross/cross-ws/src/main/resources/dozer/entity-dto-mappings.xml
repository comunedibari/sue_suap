<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <custom-converters>
            <converter type="it.wego.cross.dto.dozer.converters.XMLGC2XMLGC">
                <class-a>javax.xml.datatype.XMLGregorianCalendar</class-a>
                <class-b>javax.xml.datatype.XMLGregorianCalendar</class-b>
            </converter>
            <converter type="it.wego.cross.dto.dozer.converters.BigIntegerToInteger">
                <class-a>java.math.BigInteger</class-a>
                <class-b>java.lang.Integer</class-b>
            </converter>
        </custom-converters>      
    </configuration>
    <mapping>
        <class-a>it.wego.cross.entity.ProcessiEventi</class-a>
        <class-b>it.wego.cross.dto.dozer.ProcessoEventoDTO</class-b>
        <field custom-converter="it.wego.cross.dto.dozer.converters.FlgScadenzeCustomConverter">
            <a>processiEventiScadenzeList</a> 
            <b>flgScadenzeCustom</b>
        </field>
        <field>
            <a>processiEventiScadenzeList</a> 
            <b>processiEventiScadenzeList</b>
        </field>
    </mapping>
    
    <mapping>
        <class-a>it.wego.cross.entity.ProcessiEventiScadenze</class-a>
        <class-b>it.wego.cross.dto.dozer.ProcessoEventoDTO.ProcessoEventoScadenzaDTO</class-b>
        <field>
            <a>lkScadenze</a> 
            <b>scadenza</b>
        </field>
    </mapping>
    
    <mapping>
        <class-a>it.wego.cross.entity.Enti</class-a>
        <class-b>it.wego.cross.dto.dozer.EnteDTO</class-b>
        <field>
            <a>lkComuniList</a> 
            <b>comuniAbilitati</b>
        </field>
        <field custom-converter="it.wego.cross.dto.dozer.converters.ProcedimentiEntiToProcedimentiConverter">
            <a>procedimentiEntiList</a> 
            <b>procedimentiAttivi</b>
        </field>
        <field>
            <a>endoProcedimentiList</a> 
            <b>endoProcedimentiEntiAttivi</b>
        </field>
    </mapping>
    
    <mapping>
        <class-a>it.wego.cross.entity.ProcedimentiEnti</class-a>
        <class-b>it.wego.cross.dto.dozer.ProcedimentoEnteDTO</class-b>
        <field>
            <a>idProc</a> 
            <b>procedimento</b>
        </field>
        <field>
            <a>idEnte</a> 
            <b>ente</b>
        </field>
        <field>
            <a>idProcesso</a> 
            <b>processo</b>
        </field>
    </mapping>
    
    <mapping>
        <class-a>it.wego.cross.entity.Procedimenti</class-a>
        <class-b>it.wego.cross.dto.dozer.ProcedimentoDTO</class-b>
        <field custom-converter="it.wego.cross.dto.dozer.converters.ProcedimentiTestiListConverter">
            <a>procedimentiTestiList</a> 
            <b>descrizione</b>
        </field>
    </mapping>
    
    <mapping>
        <class-a>it.wego.cross.entity.Pratica</class-a>
        <class-b>it.wego.cross.dto.dozer.PraticaDTO</class-b>
        <field>
            <a>protocollo</a> 
            <b>protocollo.numero</b>
        </field>
        <!-- TODO: protocollo        
                <field>
                    <a>codFascicolo</a> 
                    <b>protocollo.codFascicolo</b>
                </field>
        -->        
        <field>
            <a>codRegistro</a> 
            <b>protocollo.codRegistro</b>
        </field>
        <field>
            <a>annoRiferimento</a> 
            <b>protocollo.annoRiferimento</b>
        </field>
        <field>
            <a>dataProtocollazione</a> 
            <b>protocollo.dataProtocollazione</b>
        </field>
        <field>
            <a>idStaging.idStaging</a> 
            <b>idStaging</b>
        </field>
        <field>
            <a>idModello.id</a> 
            <b>idModello</b>
        </field>
        <field>
            <a>idProcEnte.idEnte</a> 
            <b>ente</b>
        </field>
        <field>
            <a>idProcEnte.idProc</a> 
            <b>procedimento</b>
        </field>
        <field custom-converter="it.wego.cross.dto.dozer.converters.EndoProcedimentiCustomConverter">
            <a>praticaProcedimentiList</a> 
            <b>endoProcedimentiList</b>
        </field>
        
    </mapping>
    <mapping>
        <class-a>it.wego.cross.entity.DatiCatastali</class-a>
        <class-b>it.wego.cross.dto.dozer.DatiCatastaliDTO</class-b>
        <field-exclude> 
            <a>idTipoSistemaCatastale</a> 
            <b>idTipoSistemaCatastale</b> 
        </field-exclude>
        <field-exclude> 
            <a>idPratica</a> 
            <b>idPratica</b> 
        </field-exclude>
        <field-exclude> 
            <a>idTipoParticella</a> 
            <b>idTipoParticella</b> 
        </field-exclude>
        <field-exclude> 
            <a>idTipoUnita</a> 
            <b>idTipoUnita</b> 
        </field-exclude>                        
    </mapping>
    <mapping>
        <class-a>it.wego.cross.entity.DatiCatastali</class-a>
        <class-b>it.wego.cross.xml.Immobile</class-b>
        <field-exclude> 
            <a>idTipoSistemaCatastale</a> 
            <b>idTipoSistemaCatastale</b> 
        </field-exclude>
        <field-exclude> 
            <a>idPratica</a> 
            <b>idPratica</b> 
        </field-exclude>
        <field-exclude> 
            <a>idTipoParticella</a> 
            <b>idTipoParticella</b> 
        </field-exclude>
        <field-exclude> 
            <a>idTipoUnita</a> 
            <b>idTipoUnita</b> 
        </field-exclude>                        
    </mapping>
    <mapping>
        <class-a>it.wego.cross.entity.IndirizziIntervento</class-a>
        <class-b>it.wego.cross.dto.dozer.IndirizzoInterventoDTO</class-b>
        <field-exclude> 
            <a>idDug</a> 
            <b>idDug</b> 
        </field-exclude>
        <field-exclude> 
            <a>idPratica</a> 
            <b>idPratica</b> 
        </field-exclude>
    </mapping>
    <mapping>
        <class-a>it.wego.cross.entity.IndirizziIntervento</class-a>
        <class-b>it.wego.cross.xml.IndirizziIntervento</class-b>
        <field-exclude> 
            <a>idDug</a> 
            <b>idDug</b> 
        </field-exclude>
        <field-exclude> 
            <a>idPratica</a> 
            <b>idPratica</b> 
        </field-exclude>
    </mapping>
    <mapping>
        <class-a>it.wego.cross.entity.DefDatiEstesi</class-a>
        <class-b>it.wego.cross.dto.dozer.DefDatiEstesiDTO</class-b>
        <field-exclude> 
            <a>idTipoOggetto</a> 
            <b>idTipoOggetto</b> 
        </field-exclude>
    </mapping>
</mappings>