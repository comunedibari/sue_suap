<servizio>
	<nome>Servizio Tutorial 1</nome>
	<attivita>esempi</attivita>
	<sottoattivita>tutorial</sottoattivita>
	<process>it.people.process.GenericProcess</process>
	<package>it.people.fsl.servizi.esempi.tutorial.serviziotutorial1</package>
	
	<!-- includeXml se true, nel caso in cui sia inviata l'email al funzionario include l'xml come allegato -->
	<!--            se zipped, l'xml Ã¨ incluso compresso                                                    -->
	<!--            se false, non include nulla                                                             -->
	<references name="SUBMIT_PROCESS" value="BEService" shortflag="0" address="UNDEFINED" mailIncludeXml="true" />
	<references name="BEService" value="BEService" shortflag="0" address="UNDEFINED" />

	<!-- Indica se nel mail di ricevuta per il cittadino debbano essere inclusi gli allegati -->
	<receiptMailAttachment>true</receiptMailAttachment>	
	
	<!-- Indica se abilitare o meno la firma del riepilogo -->
	<signEnabled>true</signEnabled>
	
	<parameter name="parametro1" value="Io sono il valore del parametro 1"/>		
	<parameter name="parametro2" value="Io sono il valore del paramtero 2"/>
	<parameter name="disabilitaControlloDelegheInAccesso" value="true"/>
</servizio>
